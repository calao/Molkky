<html t:type="layout" title="Membre"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter">

<t:zone t:id="zone">
    <form t:id="encodeScoreForm">
        <t:zone t:id="zoneData">
    <t:jquery.datatable t:model="membreModel" t:source="membresList" t:row="currentMembre">
        <p:partie1Cell>
            <input t:type="textfield" style="display:none" t:id="idMembre" id="idMembre" value="idMembre"></input>
            <t:textfield t:id="scorePartie1" context="${currentMembre.idMembre}" value="scorePartie1"></t:textfield>
        </p:partie1Cell>
        <p:partie2Cell>
            <t:textfield t:id="scorePartie2" context="${currentMembre.idMembre}" value="scorePartie2"></t:textfield>
        </p:partie2Cell>
        <p:partie3Cell>
            <t:textfield t:id="scorePartie3" context="${currentMembre.idMembre}" value="scorePartie3"></t:textfield>
        </p:partie3Cell>
        <p:partie4Cell>
            <t:textfield t:id="scorePartie4" context="${currentMembre.idMembre}" value="scorePartie4"></t:textfield>
        </p:partie4Cell>
    </t:jquery.datatable>
        </t:zone>
        <t:submit></t:submit>
    </form>
</t:zone>

</html>
